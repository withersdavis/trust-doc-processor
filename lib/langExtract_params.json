{
  "model_type": "gemini",
  "model_id": "gemini-2.5-flash",
  "temperature": 0.1,
  "max_tokens": 4096,
  "max_char_buffer": 4000,
  "extraction_passes": 1,
  "max_workers": 10,
  "batch_length": 10,
  "use_schema_constraints": true,
  "fence_output": true,
  "debug": false,
  "citation_config": {
    "require_citations": true,
    "citation_mode": "all_data",
    "min_citation_confidence": 0.7,
    "extract_context": true,
    "context_window": 8000,
    "preserve_full_sentences": true,
    "include_location_info": true
  },
  "extraction_config": {
    "single_pass": {
      "focus": "comprehensive_extraction",
      "extract_all_mentions": true,
      "capture_context": true,
      "link_to_citations": true,
      "require_source_text": true
    }
  },
  "output_format": {
    "include_citations": true,
    "citation_format": {
      "text": "extracted_text",
      "class": "citation_type",
      "location": {
        "start": "character_position",
        "end": "character_position",
        "length": "text_length"
      },
      "context": "surrounding_text_up_to_1000_chars",
      "confidence": "0.0-1.0"
    }
  },
  "fill_missing": {
    "enabled": true,
    "default_value": "Not specified",
    "require_explicit_absence": true,
    "note": "Only use 'Not specified' when the information is genuinely not present in the document"
  },
  "validation": {
    "ensure_citations_for_key_fields": true,
    "reject_uncited_extractions": true,
    "minimum_context_length": 50,
    "maximum_context_length": 1000
  }
}